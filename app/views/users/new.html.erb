<div class="signup-image">
  <div class="row">
    <div class="col-md-10 col-md-offset-1 login-container">
      <h1 class="login-header">User Sign Up</h1>
      <%= form_for "" do |f| %>
        <div class="form-field">
          <%= f.label :first_name, class: 'login-header' %>:
          <%= f.text_field :first_name, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.label :last_name, class: 'login-header' %>:
          <%= f.text_field :last_name, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.label :phone, nil, class: 'login-header' %>:
          <%= f.email_field :phone, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.label :email, nil, class: 'login-header' %>:
          <%= f.email_field :email, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.label :password, nil, class: 'login-header' %>:
          <%= f.password_field :password, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.label :password_confirmation, nil, class: 'login-header' %>:
          <%= f.password_field :password_confirmation, class: 'form-control' %>
        </div>
        <div class="form-field">
          <%= f.submit "Log In", class: 'btn btn-primary' %>
        </div>
      <% end %>
    </div>
  </div>
</div>
