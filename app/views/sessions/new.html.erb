<h1> Log In</h1>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %><br />
    <%= password_field_tag :password, params[:email] %>
  </div>
  <div class="action"><%= submit_tag "Log In" %></div>
 <% end %>
